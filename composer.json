{
  "name": "whotrades/php-grpc",
  "description": "PHP gRPC",
  "require": {
    "grpc/grpc": "^1.15.0",
    "google/protobuf": "^v3.6.1"
  },
  "autoload": {
    "psr-4": {
      "": "generated/",
      "PhpGrpc\\": "src/"
    }
  },
  "scripts": {
    "post-package-install": [
      "git submodule init && git submodule update",
      "bash generate.sh"
    ],
    "post-package-update": [
      "git submodule init && git submodule update",
      "bash generate.sh"
    ]
  }
}
